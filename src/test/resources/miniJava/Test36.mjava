class A {
	public static void main(String[] args){
		int x = 5;
		Person p = new Person();
		p.isCool = true;
		p.n = 10;
		
		if(p.isCool)
			System.out.println(p.n);
		else
			System.out.println(x);
		
		z_test();
	}
	
	public static void z_test(){
		int z = 100;
		
		while(z < 105){
			System.out.println(z);
			z = z + 1;
		}
	}
}

class Person {
	boolean isCool;
	int n;
}
